  unless product[:title].blank? || @product = Product.find_by(name: product[:title].titleize)

    begin
      @product = Product.create!(name: product[:title].titleize, sku: product[:title].parameterize, body: product[:product_body], unit_price: product[:price].gsub('£', '').to_f) 
    rescue ActiveRecord::StatementInvalid 
      next
    end 
    if product[:images]
      
      product[:images].each do |image|
        
      begin 
        @product.product_images.create(remote_image_url: image.last)
      rescue 
        nil
      end
    end

    if @category = Category.find_by(id: 11)
      @category.products << @product
    end
    @product.save
  end

  category_name = product[:category]

  if @category = Category.find_by(name: category_name)
    @product.categorisations.create(category_id: @category.id)
  else
    @product.categories.create(name: category_name)
  end
  @product.save

